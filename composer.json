{
  "name": "tuut/storefront-child",
  "type": "wordpress-theme",
  "license": "MIT",
  "description": "Blank child theme for WooThemes StoreFront theme",
  "homepage": "https://tuut.com.br",
  "authors": [
    {
      "name": "Christian Kaisermann",
      "email": "christian@kaisermann.me",
      "homepage": "https://github.com/kaisermann"
    }
  ],
  "keywords": ["wordpress"],
  "support": {
    "issues": "https://github.com/tuut/storefront-child/issues"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    }
  },
  "require": {
    "php": ">=7",
    "composer/installers": "~1.4.0",
    "rarst/wps": "^1.1",
    "press-bits/media-library-svg-display": "^0.1.0"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.0.2"
  },
  "scripts": {
    "test": ["phpcs"],
    "fix": ["phpcbf"]
  },
  "extra": {
    "installer-paths": {
      "vendor/{$name}/": ["rarst/wps"]
    }
  }
}
